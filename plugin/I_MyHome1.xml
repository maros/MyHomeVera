<implementation>
  <files>L_MyHome1.lua</files>
  <startup>initialize</startup>
  <actionList>
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>set_status_away</name>
      <run>return set_status_away()</run>
    </action>
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>set_status_home</name>
      <run>return set_status_home()</run>
    </action>
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>set_status_vacation</name>
      <run>return set_status_vacation()</run>
    </action>
    <!-- Extra actions called by other scenes -->
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>run_automator</name>
      <run>return run_automator()</run>
    </action>
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>device_partial</name>
      <run>return device_partial(lul_settings.device,lul_settings.percentage)</run>
    </action>
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>device_open</name>
      <run>return device_open(lul_settings.device)</run>
    </action>
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>blinds_wakeup</name>
      <run>return blinds_wakeup()</run>
    </action>
    <action>
      <serviceId>urn:upnp-k1-com:serviceId:MyHome1</serviceId>
      <name>lights_set</name>
      <run>return lights_set(lul_settings.target)</run>
    </action>
  </actionList>
</implementation>
